public class Log2 {
    
    public static void table(int n) {
        int i = 1;
        while (i < 10) {
            double x = (double) i;
            System.out.println(x + "    " + Math.log(x) / Math.log(2));
            i = i + 1;
        }
    }
    
    public static void main(String[] args) {
        table(9);
    }
}